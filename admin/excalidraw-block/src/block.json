{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "excalidraw/excalidraw-block",
	"version": "0.1.1",
	"title": "Excalidraw",
	"category": "widgets",
	"description": "Block for displaying the Excalidraw documents.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "excalidraw-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"attributes": {
		"docId": {
			"type": "string"
		},
		"showTitle": {
			"type": "boolean",
			"default": true
		},
		"width": {
			"type": "integer",
			"default": 100
		},
		"alignment": {
			"type": "string",
			"default": "left"
		}
	}
}
